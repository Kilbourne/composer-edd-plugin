{
    "name": "kilbourne/composer-edd-plugin",
    "description": "Composer plugin for installing pro plugins powered by EDD software licensing",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.0",
        "vlucas/phpdotenv": "^5.0.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LubusIN\\ComposerEddPlugin\\": "src"
        }
    },
    "extra": {
        "class": "LubusIN\\ComposerEddPlugin\\Plugin",
        "plugin-modifies-downloads": true
    }
}
